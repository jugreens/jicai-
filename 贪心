//https://blog.csdn.net/qq_40916491/article/details/79944011
#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
const int N = 1e5+10;

int cmp(int a , int b)
{
    return a >b ;
}
int a ,b ,n ;
int s[55];
int main()
{
    int t;
    cin >> t ;
    while(t--)
    {
        cin >> a >> b >> n ;
        for(int i = 0 ; i < n ; i++)
            cin >> s[i];
        sort(s,s+n,cmp);
        int i ;
        for(i = 0 ; i < n ; i++)
        {
            if(b*2-s[i]>=0)
            {     
                b=b-s[i]/2;
                s[i]=s[i]%2;

            }
            else if(b!=0)
            {
                s[i]=s[i]-b*2;
                b=0;
            }

        }
        int cnt = 0 ;

        for(int i = 0 ; i < n ; i++)
        {
            cnt+=s[i];
        }

        if(a+b>=cnt)
            cout << "Yes"<<endl;
        else
            cout << "No" <<endl;
    }
}
